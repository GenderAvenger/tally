{
  "name": "@genderavenger/tally",
  "description": "Chart diversity in a room or on stage.",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "yarn cleanup && yarn build:node",
    "build:node": "tsc -p tsconfig.build.json",
    "cleanup": "rimraf dist",
    "lint": "eslint ./src --ext .ts",
    "start": "yarn build && yarn start:prod",
    "start:dev": "nodemon",
    "start:prod": "node dist/index.js",
    "test": "npm run test:unit",
    "test:unit": "jest --config=jest.config.js"
  },
  "main": "src/index.js",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GenderAvenger/tally.git"
  },
  "contributors": [
    {
      "name": "Daniel Schultz",
      "email": "slifty@gmail.com",
      "url": "https://slifty.com"
    }
  ],
  "dependencies": {},
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.16",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-sort-exports": "^0.7.0",
    "jest": "^29.0.3",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
}
